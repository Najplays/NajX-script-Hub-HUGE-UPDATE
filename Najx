local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Najx", "Ocean")
local Player = Window:NewTab("Player")
local Main = Window:NewTab("Main")
local PlayerSection = Player:NewSection("PlayerSection")
local MainSection = Main:NewSection("MainSection")

PlayerSection:NewSlider("Walkspeed", "Walkspeed Increases you're walk", 300, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)
PlayerSection:NewSlider("JumpPower", "JmumPower increases you're jump", 300, 50, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)

MainSection:NewDropdown("Infinte Yeild", "Enables Infinte Yeild", {"Infinte Yeild"}, function(v)
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
MainSection:NewDropdown("Hades Hub", "Enables Hades Hub", {"Hades Hub"}, function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HadesRblx/src/master/HadesHub.lua", true))()
end)
MainSection:NewDropdown("CMD-X", "Enables CMD-X", {"CMD-X"}, function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)

MainSection:NewDropdown("Pendulum Hub", "Enables Pendulum Hub", {"Pendulum Hub"}, function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Tescalus/Pendulum-Hubs-Source/main/Pendulum%20Hub%20V5.lua"))()
end)
MainSection:NewDropdown("Sky.gg", "Enables Sky.gg", {"Sky.gg"}, function(v)
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-SkaysGG-2095"))()
end)
MainSection:NewDropdown("FunkyFriday", "Enables FunkyFriday script", {"FunkyFriday"}, function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZoinkyPoinkie/FunkyFridayDevTools/main/Un-Obfuscated",true))()
end)
MainSection:NewDropdown("OwlHub", "Enables OwlHub", {"OwlHub"}, function(v)
    loadstring(game:HttpGet("https://athoi21.xyz/owlhub"))();
end)
MainSection:NewDropdown("Syntax V2", "Enables Syntax V2 [and key is SyntaxBestHub i think?]", {"Syntax V2"}, function(v)
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/Vallater/SyntaxV2/b7a88a7b84174e3c2220c7a8ca477e40699ddd2c/Syntaxontop'),true))()
end)
